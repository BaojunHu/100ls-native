    "eslint": "9.23.0",
    "jsdom": "26.0.0",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  },
